{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "HandsOnLab",
  "type": "object",
  "required": [
    "lab_id",
    "title",
    "objectives",
    "setup_instructions",
    "implementation_guide",
    "starter_code",
    "solution_code",
    "expected_outputs",
    "troubleshooting_guide",
    "nvidia_platforms_used"
  ],
  "properties": {
    "lab_id": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "objectives": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "string"
      }
    },
    "setup_instructions": {
      "type": "string",
      "minLength": 1
    },
    "implementation_guide": {
      "type": "string",
      "minLength": 1
    },
    "starter_code": {
      "type": "object",
      "minProperties": 1,
      "additionalProperties": {
        "type": "string"
      }
    },
    "solution_code": {
      "type": "object",
      "additionalProperties": {
        "type": "string"
      }
    },
    "expected_outputs": {
      "type": "object",
      "minProperties": 1
    },
    "troubleshooting_guide": {
      "type": "string",
      "minLength": 1
    },
    "nvidia_platforms_used": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "string"
      }
    }
  }
}
